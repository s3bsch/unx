[user]
	name = changeme
	email = changeme
[alias]
	a = add
	br = branch
        bm = branch --merged     # list branches merged into HEAD
        bn = branch --no-merged  # list branches not merged into HEAD
	cm = commit
	co = checkout
	d = diff
	dc = diff --cached
	st = status
	s = status
	l = log
	lol = log --oneline -10
	graph = log --graph --oneline --all
[color]
	ui = auto
[push]
	default = simple
